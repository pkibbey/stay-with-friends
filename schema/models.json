{
  "entities": {
    "User": {
      "table": "users",
      "fields": {
        "id": { "type": "string", "primary": true, "autoincrement": true },
        "email": { "type": "string", "unique": true, "notNull": true },
        "name": { "type": "string", "nullable": true },
        "email_verified": { "type": "datetime", "nullable": true },
        "image": { "type": "string", "nullable": true },
        "created_at": { "type": "datetime", "default": "CURRENT_TIMESTAMP" }
      }
    },
    "Host": {
      "table": "hosts",
      "fields": {
        "id": { "type": "string", "primary": true, "autoincrement": true },
        "user_id": { "type": "string", "nullable": true },
        "name": { "type": "string", "notNull": true },
        "location": { "type": "string", "nullable": true },
        "description": { "type": "string", "nullable": true },
        "address": { "type": "string", "nullable": true },
        "city": { "type": "string", "nullable": true },
        "state": { "type": "string", "nullable": true },
        "zip_code": { "type": "string", "nullable": true },
        "country": { "type": "string", "nullable": true },
        "latitude": { "type": "real", "nullable": true },
        "longitude": { "type": "real", "nullable": true },
        "amenities": { "type": "json", "jsonType": "string[]", "nullable": true },
        "house_rules": { "type": "string", "nullable": true },
        "check_in_time": { "type": "string", "nullable": true },
        "check_out_time": { "type": "string", "nullable": true },
        "max_guests": { "type": "integer", "nullable": true },
        "bedrooms": { "type": "integer", "nullable": true },
        "bathrooms": { "type": "integer", "nullable": true },
        "photos": { "type": "json", "jsonType": "string[]", "nullable": true },
        "created_at": { "type": "datetime", "default": "CURRENT_TIMESTAMP" },
        "updated_at": { "type": "datetime", "default": "CURRENT_TIMESTAMP" }
      }
    },
    "Availability": {
      "table": "availabilities",
      "fields": {
        "id": { "type": "string", "primary": true, "autoincrement": true },
        "host_id": { "type": "string", "notNull": true },
        "start_date": { "type": "string", "notNull": true },
        "end_date": { "type": "string", "notNull": true },
        "status": { "type": "string", "default": "available" },
        "notes": { "type": "string", "nullable": true }
      }
    },
    "BookingRequest": {
      "table": "booking_requests",
      "fields": {
        "id": { "type": "string", "primary": true, "autoincrement": true },
        "host_id": { "type": "string", "notNull": true },
        "requester_id": { "type": "string", "notNull": true },
        "start_date": { "type": "string", "notNull": true },
        "end_date": { "type": "string", "notNull": true },
        "guests": { "type": "integer", "notNull": true },
        "message": { "type": "string", "nullable": true },
        "status": { "type": "string", "default": "pending" },
        "response_message": { "type": "string", "nullable": true },
        "responded_at": { "type": "datetime", "nullable": true },
        "created_at": { "type": "datetime", "default": "CURRENT_TIMESTAMP" }
      }
    },
    "Connection": {
      "table": "connections",
      "fields": {
        "id": { "type": "string", "primary": true, "autoincrement": true },
        "user_id": { "type": "string", "notNull": true },
        "connected_user_id": { "type": "string", "notNull": true },
        "relationship": { "type": "string", "nullable": true },
        "status": { "type": "string", "default": "pending" },
        "created_at": { "type": "datetime", "default": "CURRENT_TIMESTAMP" }
      }
    },
    "Invitation": {
      "table": "invitations",
      "fields": {
        "id": { "type": "string", "primary": true, "autoincrement": true },
        "inviter_id": { "type": "string", "notNull": true },
        "invitee_email": { "type": "string", "notNull": true },
        "invitee_name": { "type": "string", "nullable": true },
        "message": { "type": "string", "nullable": true },
        "token": { "type": "string", "unique": true, "notNull": true },
        "status": { "type": "string", "default": "pending" },
        "expires_at": { "type": "datetime", "notNull": true },
        "accepted_at": { "type": "datetime", "nullable": true },
        "created_at": { "type": "datetime", "default": "CURRENT_TIMESTAMP" }
      }
    }
  }
}
